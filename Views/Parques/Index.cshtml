@model IEnumerable<EFGetStarted.AspNet5.NewDb.Models.Parque>

@{
    ViewBag.Title = "Parques";
}

<h2>Parques</h2>

<p>
    <a asp-controller="Parques" asp-action="Create">Create New</a>
</p>

<table class="table">
    <tr>
        <th>Id</th>
        <th>Nombre</th>
        <th>Barrio</th>
        <th>Dirección</th>
        <th>Área</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ParqueId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Barrio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Area)
            </td>
        </tr>
    }
</table>